function fish_user_key_bindings
		 bind \cu backward-char
		 bind \ch forward-char
		 bind \ce history-search-backward
		 bind \ct history-search-forward
		 bind \co beginning-of-line
		 bind \cn end-of-line
		 bind \ca accept-autosuggestion
		 bind \eu backward-word
		 bind \eh forward-word
		 bind \cd delete-char
		 bind \ck kill-whole-line
end
